#!/usr/bin/env ruby
# frozen_string_literal: true

require "bundler/setup"
require "difftastic"

differ = Difftastic::Differ.new(
	color: :always,
	left_label: "Expected",
	right_label: "Actual"
)

puts "Diff with labels:"
puts differ.diff_objects(
	{ status: "pending", count: 5 },
	{ status: "completed", count: 10 }
)
